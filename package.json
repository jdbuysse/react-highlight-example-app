{
  "name": "Power",
  "version": "1.0.0",
  "description": "Highlightable demo page source",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist lint:tools",
    "start": "npm-run-all --parallel open:src",
    "start:e2e": "npm run build:test; babel-node tools/testServer.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "open:test": "babel-node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "clean-dist:test": "npm run remove-dist:test && mkdir dist_test",
    "remove-dist:test": "node_modules/.bin/rimraf ./dist_test",
    "build:html": "NODE_ENV='production' babel-node tools/buildHtml.js",
    "build:test": "npm run clean-dist:test; NODE_ENV='test' babel-node tools/buildHtml.js; babel-node tools/buildTest.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node tools/build.js",
    "build:staging": "npm run clean-dist; NODE_ENV='staging' babel-node tools/buildHtml.js;NODE_ENV='staging' babel-node tools/build.js",
    "test": "mocha tools/testSetup.js src/**/**/*.spec.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-runtime": "6.11.6",
    "classnames": "2.2.4",
    "flexboxgrid": "6.3.0",
    "highlightable": "1.0.3",
    "immutable": "3.8.1",
    "js-md5": "0.4.1",
    "material-ui": "0.15.4",
    "object-assign": "4.0.1",
    "rc-tooltip": "3.3.2",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-immutable-proptypes": "2.1.0",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "substyle": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "browser-sync": "2.11.2",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "connect-history-api-fallback": "1.2.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.7.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-import": "1.4.0",
    "eslint-plugin-react": "4.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "json-loader": "0.5.4",
    "node-sass": "3.4.2",
    "npm-run-all": "1.7.0",
    "react-addons-test-utils": "15.0.1",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "webpack": "1.12.15",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
